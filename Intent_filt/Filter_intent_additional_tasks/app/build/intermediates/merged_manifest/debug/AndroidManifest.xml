<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.filter_intent_additional_tasks"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Filter_intent_additional_tasks" >
        <activity
            android:name="com.example.filter_intent_additional_tasks.Align_textActivity"
            android:exported="true" />
        <activity
            android:name="com.example.filter_intent_additional_tasks.Edit_colorActivity"
            android:exported="true" />
        <activity
            android:name="com.example.filter_intent_additional_tasks.EditorActivity"
            android:exported="true" />
        <activity
            android:name="com.example.filter_intent_additional_tasks.Camera_showActivity"
            android:exported="true" />
        <activity
            android:name="com.example.filter_intent_additional_tasks.Action_systemActivity"
            android:exported="true" />
        <activity
            android:name="com.example.filter_intent_additional_tasks.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>